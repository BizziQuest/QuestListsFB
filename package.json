{
  "name": "questlistsfb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode development",
    "build:watch": "vue-cli-service build --mode development --watch --no-clean",
    "ci:e2e": "firebase emulators:exec --only firestore 'yarn test:e2e --headless --mode=development'",
    "ci:e2e:clean": "firebase emulators:exec --only firestore 'yarn test:e2e --headless --mode=development'",
    "ci:unit": "firebase emulators:exec --only firestore 'yarn test:unit'",
    "dev": "run-p fb:emulate serve",
    "fb:deploy": "firebase deploy",
    "fb:dev": "firebase emulators:exec --only firestore --import tests/data 'yarn serve --open'",
    "fb:dev:clean": "firebase emulators:exec --only firestore 'yarn serve --open'",
    "fb:e2e": "firebase emulators:exec --only firestore 'yarn test:e2e --mode=development'",
    "fb:e2e:clean": "firebase emulators:exec --only firestore 'yarn test:e2e --mode=development'",
    "fb:emulate": "firebase emulators:start --only firestore,auth --import tests/data",
    "fb:emulate:save": "firebase emulators:export --force tests/data",
    "fb:emulate:stop": "firebase emulators:stop",
    "fb:login": "firebase login --no-localhost",
    "fb:serve": "firebase serve --only firestore,auth,hosting --host 0.0.0.0",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "serve:build": "run-s build:dev watch",
    "test": "yarn test:unit && yarn test:e2e",
    "ui": "vue ui"
  },
  "dependencies": {
    "@mdi/font": "^5.8.55",
    "core-js": "^3.8.0",
    "dotenv": "^8.2.0",
    "firebase": "8.0.2",
    "material-design-icons-iconfont": "^6.1.0",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "*",
    "slugify": "^1.4.6",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify-preset-rally": "^1.0.4",
    "vue-cli-plugin-vuetify-preset-reply": "^1.0.4",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.4.9",
    "vuetify": "^2.3.19",
    "vuex": "^3.6.0",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@vue/cli": "^4.5.9",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-e2e-cypress": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/test-utils": "1.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.9.0",
    "firebase-tools": "^8.16.2",
    "sass": "^1.29.0",
    "sass-loader": "^8",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0"
  },
  "license": "MIT",
  "title": "QuestLists"
}
