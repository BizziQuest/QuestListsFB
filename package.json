{
  "name": "questlistsfb",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --mode production",
    "ci": "yarn lint && yarn test",
    "dev": "yarn fb:emulate; yarn serve --open",
    "fb:deploy": "firebase deploy",
    "fb:dev": "firebase emulators:exec --only firestore 'yarn serve --open'",
    "fb:emulate": "firebase emulators:start --only firestore --import tests/data",
    "fb:emulate:save": "firebase emulators:export --force tests/data",
    "fb:login": "firebase login --no-localhost",
    "test": "yarn test:unit && yarn test:e2e",
    "ui": "vue ui"
  },
  "dependencies": {
    "@firebase/auth": "^0.14.9",
    "@mdi/font": "^5.4.55",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "firebase": "^7.17.1",
    "material-design-icons-iconfont": "^5.0.1",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify-preset-reply": "^1.0.4",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.7",
    "vuex": "^3.5.1",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@vue/cli": "4.5.0",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-e2e-cypress": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.9.0",
    "firebase-tools": "^8.6.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0"
  }
}
